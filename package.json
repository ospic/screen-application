    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-runtime": "^6.26.0",
    "dotenv": "^16.0.1",
    "sass": "^1.53.0",
    "sass-loader": "10.1.1"
  }
